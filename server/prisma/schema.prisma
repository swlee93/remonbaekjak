generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Task {
  id       Int         @id @default(autoincrement())
  name     String
  type     String
  description String
  createdBy String
  createdAt DateTime   @default(now())
  updatedAt DateTime   @default(now()) @updatedAt
}

model Settings {
  userId                              String    @id @default("SYSTEM")
  github_personal_access_token        String    @default("")
}
